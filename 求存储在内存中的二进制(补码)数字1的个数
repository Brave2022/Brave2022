#include <stdio.h>
int main()
{
	int a = 0;
	scanf("%d", &a);
	int i = 0;
	int count = 0;
	//
	//000000000000000000000000000000000101 5  正数的原码反码补码相同
	//000000000000000000000000000000000001 1  按位与操作
	//000000000000000000000000000000000001 1
	//100000000000000000000000000000000001 -1(原码)
	//111111111111111111111111111111111111 -1(补码)

	for (i = 0; i < 32; i++)
	{
		if (1==((a >> i) & 1))
		{
			count++;
		}
	}
	printf("%d", count);
	return 0;
}
