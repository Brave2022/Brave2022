#include <math.h>
#include <stdio.h>

int main()
{
	float a = 0.0, b = 0.0, c = 0.0;
	float x1 = 0.0, x2 = 0.0,x=0.0;
	scanf("%f%f%f", &a, &b, &c);
	if (a != 0 &&( b *b) > (4 * a * c))
	{
		x = sqrt(b * b - 4 * a * c);
		x1 = ( - b + x) / (2 * a);
		x2 = ( - b - x) / (2 * a);
		printf("%.2f %.2f", x1, x2);
	}
	return 0;
}
